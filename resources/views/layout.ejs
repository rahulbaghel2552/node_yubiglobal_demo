<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Yubi-Global</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />
    <link
    rel="stylesheet"
    href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css"
  />
    <link rel="stylesheet" href="/css/style.css" />
  </head>

  <body>
    <div class="wrapper">
      <header>
        <div class="cstm_container">
          <div class="header_row">
            <a href="/" class="logo">
              <img src="/images/logo.png" alt="" />
            </a>
            <nav>
              <ul class="nav_menu">
                <li><a href="/">Home</a></li>
                <li><a href="/userDetails">Users</a></li>
               
                <li><a href="/contact">Contact</a></li>
              </ul>
              <ul class="form_sec">
                <li><a href="/login">Sign in</a></li>
                <li><a href="/register" class="reg_btn">Register</a></li>
              </ul>
            </nav>
            <button id="tog_menu" type="button" class="mob_menu" wfd-id="300">
              <i class="fa fa-bars" aria-hidden="true"></i>
            </button>
          </div>
        </div>
      </header>


    <%- body %>


      <footer>
        <div class="cstm_container">
          <div class="footer_top">
            <a href="#" class="footer_logo">
              <img src="/images/logo.png" alt="" />
            </a>
            <ul class="footer_form">
              <li><a href="/login">Sign in</a></li>
              <li><a href="/register">Register</a></li>
            </ul>
          </div>
          <div class="footer_row">
            <div class="footer_col">
              <h3>About.</h3>
              <ul>
                <li><a href="#">About</a></li>
                <li><a href="#">Browser</a></li>
                <li><a href="/contact">Contact Us</a></li>
              </ul>
            </div>
            <div class="footer_col">
              <h3>Help.</h3>
              <ul>
                <li><a href="#">Contact & Support</a></li>
                <li><a href="#">Privacy Policy</a></li>
                <li><a href="#">Terms of use</a></li>
              </ul>
            </div>
            <div class="footer_col footer_col_ext">
              <h3>Explore.</h3>
              <ul class="cash_manage_list">
                <li>
                  <div class="cash_list_img">
                    <img src="/images/banking-icon.png" alt="" />
                  </div>
                  Banking
                </li>
                <li>
                  <div class="cash_list_img">
                    <img src="/images/escrow-icon.png" alt="" />
                  </div>
                  Escrow
                </li>
                <li>
                  <div class="cash_list_img">
                    <img src="/images/payments-icon.png" alt="" />
                  </div>
                  B2B Payment
                </li>
              </ul>
            </div>
          </div>
          <p class="copyright">
            @2019 <a href="">Yubi Global. All Right Reserved</a>
          </p>
        </div>
      </footer>
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="/js/client.js"></script>
    <script>
      $("#tog_menu").click(function () {
        $("nav").slideToggle();
      });
    </script>
  </body>
</html>
